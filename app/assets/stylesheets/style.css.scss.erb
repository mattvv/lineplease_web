/* Global properties ======================================================== */

body {
  background: url("<%= asset_path "bg.gif"%>") 50% 0 repeat;
  font-family: Arial, Helvetica, sans-serif;
  font-size: 12px;
  line-height: 20px;
  color: #aaaaaa;
  min-width: 1000px; }

html, body {
  height: 100%; }

p {
  padding-bottom: 20px; }

a {
  color: #d52d00;
  outline: none;
  &:hover {
    text-decoration: none; } }

